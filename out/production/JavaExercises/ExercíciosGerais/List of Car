import java.util.ArrayList;
import java.util.List;
import java.util.Locale;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Locale.setDefault(Locale.US);
        Scanner sc = new Scanner(System.in);

        List<Car> list = new ArrayList<>();

        System.out.println("Enter the number of car in the List:");
        int num = sc.nextInt();

        for (int i = 1; i <= num; i++) {
            System.out.println("Car #" + i + ":");
            sc.nextLine();
            System.out.printf("Model of car:\n");
            String model = sc.nextLine();
            System.out.printf("Brand of car:\n");
            String brand = sc.nextLine();
            System.out.printf("Year of car:\n");
            int year = sc.nextInt();

            list.add(new Car(model, brand, year));
        }

        System.out.println("\nList of cars:");
        for(Car car : list){
            System.out.println(car);
        }

        sc.close();
    }
}

public class Car {

    private String model;
    private String brand;
    private int year;

    public Car(String model, String brand, int year) {
        this.model = model;
        this.brand = brand;
        this.year = year;
    }

    public String getModel() {
        return model;
    }

    public void setModel(String model) {
        this.model = model;
    }

    public String getBrand() {
        return brand;
    }

    public void setBrand(String brand) {
        this.brand = brand;
    }

    public int getYear() {
        return year;
    }

    public void setYear(int year) {
        this.year = year;
    }

    @Override
    public String toString() {
        return String.format("Model: %s, Brand: %s, Year: %d", model, brand, year);
    }
}
